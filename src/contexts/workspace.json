{
    "uri": "workspace",
    "title": "Workspace",
    "base": "_base",
    "layers": [
      {
        "title": "Vector dataSource",
        "visible": true,
        "sourceOptions": {
          "url": "/swtq?service=WFS&request=GetFeature&version=1.1.0&typename=telephone_urg&outputFormat=geojson",
          "type": "vector",
          "distance": 50
        }
      },
      {
        "title": "Cluster dataSource",
        "visible": false,
        "sourceOptions": {
          "url": "/swtq?service=WFS&request=GetFeature&version=1.1.0&typename=telephone_urg&outputFormat=geojson",
          "type": "cluster",
          "distance": 50
        }
      },
      {
        "title": "WFS",
        "visible": false,
        "sourceOptions": {
          "type": "wfs",
          "url": "/swtq",
          "queryable": true,
          "params": {
            "featureTypes": "telephone_urg",
            "fieldNameGeometry": "geometry",
            "maxFeatures": 10000,
            "version": "1.1.0",
            "outputFormat": "geojson",
            "outputFormatDownload": "shp"
          },
          "sourceFields": [
            { "alias": "Identifiant", "name": "desc_local" , "excludeFromOgcFilters": true },
            { "alias": "Direction", "name": "dirrte", "allowedOperatorsType": "basic"},
            { "alias": "Responsable", "name": "orgresp", "allowedOperatorsType": "all"},
            { "alias": "Emplacement", "name": "emplacemen" }
        ],
  
        "ogcFilters": {
          "enabled": true,
          "editable": true,
          "allowedOperatorsType": "all",
          "filters": {
            "operator": "PropertyIsEqualTo",
            "propertyName": "orgresp",
            "expression": "Bell"
          },
          "k2pushButtons": {
            "groups": [
              {"title": "Group 1 Title","name": "1","ids": ["id1","id2"]}
            ],
            "bundles": [
              {
                "id": "id1",
                "logical": "Or",
                "buttons": [
                  {
                    "title": "Bell",
                    "enabled": true,
                    "filters": {
                      "operator": "PropertyIsEqualTo",
                      "propertyName": "orgresp",
                      "expression": "Bell"
                    }
                  },
                  {
                    "title": "MTQ",
                    "enabled": false,
                    "filters": {
                      "operator": "PropertyIsEqualTo",
                      "propertyName": "orgresp",
                      "expression": "MTQ"
                    }
                  }
                ]
              }
            ]
        }}
        }
      }
     ,
      {
        "title": "MVT dataSource",
        "visible": false,
        "sourceOptions": {
          "featureClass": "feature" ,
          "type": "mvt",
          "queryable": true,
          "url": "/swtq?mode=tile&tilemode=gmap&tile={x}+{y}+{z}&layers=telephone_urg&map.imagetype=mvt"
        }
      }],
    "toolbar": [
      "searchResults",
      "contextManager",
      "mapTools",
      "ogcFilter",
      "measurer",
      "directions",
      "catalog",
      "importExport",
      "print",
      "shareMap",
      "about"
    ]
  }
  